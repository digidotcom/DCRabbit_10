_wifig_nodebug
int os_get_random(unsigned char __far *buf, size_t len)
{
   int i;
   unsigned long r;

   for (i = 0; i < len; i += 4) {
      r = seed_getbits(32);   // Get 32 random bits
      _f_memcpy(buf + i, &r, len - i >= 4 ? 4 : len - i);
   }

	return 0;   // Always succeeds.
}

