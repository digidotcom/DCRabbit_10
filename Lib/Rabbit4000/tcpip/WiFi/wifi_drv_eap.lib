	wpa_supplicant_deauthenticate(XXX_priv, &wpa_s, REASON_DEAUTH_LEAVING);
	wpa_driver_wln_events_deinit(&wpa_s, &wpa_s);
#ifdef WPA_USE_EAP
	eap_peer_unregister_methods();
#endif
	wpa_supplicant_cleanup(&wpa_s);

	eloop_terminate();
	eloop_destroy();
}

