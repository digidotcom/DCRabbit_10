/*** BeginHeader os_sleep */
void os_sleep(os_time_t sec, os_time_t usec);
/*** EndHeader */
_wifig_nodebug
void os_sleep(os_time_t sec, os_time_t usec)
{
   os_time_t msec;
#ifndef DONT_WARN_OS_SLEEP
   _WPA_PRINTF((MSG_WARNING, "!!!!!!! Using os_sleep() !!!!!!!!!"));
#endif
   msec = 0;
	if (sec)
      msec += sec * 1000;
	if (usec)
      msec += usec / 1024 + 1;
   msec += MS_TIMER;
   while (MS_TIMER - msec < 0);
}

