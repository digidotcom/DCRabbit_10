   exx
   ld		a,c
   exx
   ld		c',c
   ld		c,a
	ex		jk',hl
   ex		de,hl
   ex		jk,hl
   ld		a,h
   exx
   ld		h',b
	exx
   ld		b',d
   ld		d,b
   ld		b,a
   ld		a,L
   ld		L,e
   ld		e,a
   ex		jk,hl
