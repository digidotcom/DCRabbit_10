// ConsoleCommand structure
typedef struct {
	char* command;
	int (*cmdfunc)();
	long helptext;
} ConsoleCommand;

// ConsoleError structure
typedef struct {
	unsigned int code;
	char* message;
} ConsoleError;

// ConsoleIO structure
typedef struct {
	long param;			// passed to open and close function pointers
	int (*open)(long param);
	void (*close)(long);
	int (*tick)();
	int (*puts)(char *str);
	int (*rdUsed)(void);
	int (*wrUsed)(void);
	int (*wrFree)(void);
	int (*read)(void __far *data, int length, unsigned long timeout);
	int (*write)(const void __far *data, int length);
} ConsoleIO;

