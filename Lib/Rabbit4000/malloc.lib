/*** BeginHeader mspace_usable_size */
m_size_t mspace_usable_size(m_voidptr mem);
/*** EndHeader */
_malloc_debug
m_size_t mspace_usable_size(m_voidptr mem) {
  mchunkptr p;
  if (mem != 0) {
    p = mem2chunk(mem);
    if (cinuse(p))
      return chunksize(p) - overhead_for(p);
  }
  return 0;
}

