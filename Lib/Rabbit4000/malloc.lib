#define    RELEASE_MORECORE_LOCK()
/*** EndHeader */
/* Get memory from system using MORECORE */
// Rabbit does not support this, so return failure.
_malloc_debug
__static m_voidptr sys_alloc(mstate m, m_size_t nb) {
