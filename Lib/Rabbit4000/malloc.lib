#define MALLOC_PRE_ACTION(M)  ((GLOBALLY_INITIALIZE() || use_lock(M))? \
											ACQUIRE_LOCK(&(M)->mutex) : 0)
#define MALLOC_POST_ACTION(M) { if (use_lock(M)) RELEASE_LOCK(&(M)->mutex); }
#else /* MALLOC_USE_LOCKS */

